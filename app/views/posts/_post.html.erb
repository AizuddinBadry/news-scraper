<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= post.description %>
  </p>

  <p>
    <strong>Link:</strong>
    <%= post.link %>
  </p>

  <p>
    <strong>Published time:</strong>
    <%= post.published_time %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= post.category %>
  </p>

  <p>
    <strong>Group:</strong>
    <%= post.group %>
  </p>

</div>
